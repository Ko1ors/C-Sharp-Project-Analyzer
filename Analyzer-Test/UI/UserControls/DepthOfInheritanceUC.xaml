<UserControl x:Class="Analyzer_Test.UI.UserControls.DepthOfInheritanceUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Analyzer_Test.UI.UserControls" xmlns:ui="clr-namespace:Analyzer_Test.UI"
             mc:Ignorable="d" 
             Height="150" Width="190">
    <GroupBox  Background="White" BorderThickness="1,1,1,1" Header="Depth of Inheritance" FontFamily="Arial Black" FontSize="15" Padding="10" BorderBrush="Black">
        <Grid>
            <ui:RadialProgressBar Grid.Row="1" x:Name="CircleProgressBar" Height="90" Width="90" Value="100"  IndicatorBrush="#FF5046A2"  ProgressBorderBrush="Gainsboro" BorderBrush="White" BackgroundBrush="#FFBDBDBD"/>
        </Grid>
    </GroupBox>
</UserControl>
